{
  "name": "checkme",
  "description": "A simple todo cli app",
  "version": "1.0.0",
  "author": "Nguyễn Việt Hưng @ZeroX-DG",
  "bin": {
    "checkme": "./bin/run"
  },
  "bugs": "https://github.com/ZeroX-DG/checkme/issues",
  "dependencies": {
    "@oclif/command": "^1.4.33",
    "@oclif/config": "^1.6.31",
    "@oclif/plugin-help": "^2.0.5",
    "@types/cli-table": "^0.3.0",
    "@types/inquirer": "0.0.41",
    "chalk": "^2.4.1",
    "cli-table": "^0.3.1",
    "inquirer": "^6.0.0",
    "tslib": "^1.9.2"
  },
  "devDependencies": {
    "@oclif/dev-cli": "^1.13.33",
    "@oclif/test": "^1.1.0",
    "@oclif/tslint": "^1.1.2",
    "@types/chai": "^4.1.4",
    "@types/mocha": "^5.2.2",
    "@types/node": "^10.3.3",
    "chai": "^4.1.2",
    "globby": "^8.0.1",
    "mocha": "^5.1.1",
    "nyc": "^12.0.2",
    "ts-node": "^6.1.1",
    "tslint": "^5.10.0",
    "typescript": "^2.9.2"
  },
  "engines": {
    "node": ">=8.0.0"
  },
  "files": [
    "/bin",
    "/lib",
    "/npm-shrinkwrap.json",
    "/oclif.manifest.json"
  ],
  "homepage": "https://github.com/ZeroX-DG/checkme",
  "keywords": [
    "oclif"
  ],
  "license": "MIT",
  "main": "lib/index.js",
  "oclif": {
    "commands": "./lib/commands",
    "bin": "checkme",
    "plugins": [
      "@oclif/plugin-help"
    ]
  },
  "repository": "ZeroX-DG/checkme",
  "scripts": {
    "postpack": "rm -f oclif.manifest.json npm-shrinkwrap.json",
    "posttest": "tsc -p test --noEmit && tslint -p test -t stylish",
    "prepack": "rm -rf lib && tsc && oclif-dev manifest && oclif-dev readme && npm shrinkwrap",
    "prepare": "rm -rf lib && tsc",
    "test": "nyc mocha --forbid-only \"test/**/*.test.ts\"",
    "version": "oclif-dev readme && git add README.md"
  },
  "types": "lib/index.d.ts"
}
